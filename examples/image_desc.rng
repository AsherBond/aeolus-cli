<?xml version="1.0"?>
<!-- A Relax NG schema for image description xml -->
<grammar xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <ref name='image'/>
  </start>

  <define name='image'>
    <element name='image'>
      <element name='name'>
        <text/>
      </element>
    </element>
  </define>

</grammar>
